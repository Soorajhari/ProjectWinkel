              <div class="row">
                <div class="col-lg-12 grid-margin stretch-card">
                  <div class="card">
                    <div class="card-body">
                      <h4 class="card-title">Banner</h4>
                      <div class="table-responsive">
                        <table class="table">
                          <thead>
                            <tr>
                              <th>ID</th>
                              <th>Title</th>
  
                              <th>Button</th>
                              <th>Text</th>
                              <th>Description</th>
                              <th>Expiry Date</th>
                              <th>Active</th>
                              <th>Image</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <% banners.forEach(function(item, index) { %>
                              
                                <tr>
                                  <td><%= index + 1 %></td>
                                  <td><%= item.title %></td> 
                                  <td><%= item.button %></td>
                                  <td><%= item.text %></td>
                                  <td><%= item.description %></td>
                                  <td>
                                    <% const dateStr = item.ExpiryAt.toISOString(); %>
                                    <%= dateStr.slice(0, 10) %>
                                  </td>
                                  
                                  <td><%= item.active %></td>
                                 <td>
                                  <% if (item.imagepath && Array.isArray(item.imagepath)) { %>
                                    <% item.imagepath.forEach(function(image, imageIndex) { %>
                                      <% const imagePath = image.replace('public', ''); %>
                                      <img class="image" style="width: 80px;height: 20vh; border-radius: 0px;" src="<%= imagePath %>" alt="no">
                                    <% }) %>
                                  <% } %>
                                  
                                </td>
                                  <td>
                                    <a href="banner/editbanner/<%=item._id%>" class="btn btn-warning">Edit</a>

                                   
                                  </td>
                                </tr>
                             
                            <% }) %>
                          </tbody>
                        </table>
                      </div>
                    </div>
