<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<script>
$(document).ready(function() {
  $('.block-toggle').click(function(event) {
    event.preventDefault();

    var button = $(this);
    var userId = button.data('user-id');
    var isBlocked = button.data('blocked');
    var index = button.data('index');

    // Send AJAX request to block/unblock user
    $.ajax({
      url: `/admin/users/${userId}/block`,
      method: 'PUT',
      success: function(response) {

        // Toggle the blocked data attribute and button text/class
        button.data('blocked', !isBlocked);
        if (!isBlocked) {
          button.text('Unblock');
          button.removeClass('btn-outline-warning');
          button.addClass('btn-warning');
        } else {
          button.text('Block');
          button.removeClass('btn-warning');
          button.addClass('btn-outline-warning');
        }
      },
      error: function(error) {
        console.log(error);
      }
    });
  });
});
</script>
