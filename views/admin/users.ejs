            <!-- <p class="card-description">
              Add class <code>.table</code>
            </p> -->
            <div class="table-responsive">
              <table class="table">
                <thead>
                  <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>MObile</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% user.forEach(function(user, index) { %>
                    <tr>
                      <td><%= user.name %></td>
                      <td><%= user.email %></td>
                      <td><%= user.mobile %></td>
                      <td>
                        <% if (user.is_block) { %>
                          <button class="btn btn-warning block-toggle" data-user-id="<%= user._id %>" data-blocked="<%= user.is_block %>" data-index="<%= index %>">Unblock</button>
                        <% } else { %>
                          <button class="btn btn-primary block-toggle" data-user-id="<%= user._id %>" data-blocked="<%= user.is_block %>" data-index="<%= index %>">Block</button>
                        <% } %>
                      </td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
