                  </li>
                  <% for (let i = 1; i < totalPages; i++) { %>
                  <% const isActive = i === currentPage; %>
                  <li class="page-item <%= isActive ? 'active' : '' %>">
                    <a class="page-link" href="/admin/orderadmin?page=<%= i %>">
                      <span style="font-size: 18px;"><%= i %></span>
                    </a>
                  </li>
                  <% } %>
                  <li class="page-item">
                    <a class="page-link" href="/admin/orderadmin?page=<%= totalPages %>">&gt;</a>
                  </li>
                </ul>
              </div>
            </div>
